{
    "name": "geis",
    "version": "1.0",
    "description": "Get entities from google searches",
    "permissions": ["storage", "tabs", "*://*/*"],
    "background": {
	"scripts": ["background.js"],
	"persistent": false
    },
    "browser_action": {
	"default_popup": "popup.html"
    },
    "manifest_version": 2
}
